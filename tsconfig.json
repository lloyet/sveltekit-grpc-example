{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"paths": {
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"],
			"$stores": ["./src/lib/stores"],
			"$env": ["./src/lib/env"],
			"$components": ["./src/lib/components"],
			"$components/*": ["./src/lib/components/*"],
			"$utils": ["./src/lib/utils"],
			"$utils/*": ["./src/lib/utils/*"],
			"$proto": ["./src/lib/proto"],
			"$proto/*": ["./src/lib/proto/*"]
		}
	}
}
